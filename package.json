{
  "name": "tkngen-api",
  "version": "0.1.0",
  "description": "tkngen's api. This is the backend responsible for making and deploying ERC20 tokens and BEP20 tokens.",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "chmod +x ./.test.sh && ./.test.sh",
    "tests": "mocha -r ts-node/register tests/.test.ts --timeout 900000",
    "dev:start": "yarn run ts-node ./lib/server.ts",
    "dev:start_hr": "nodemon",
    "chain": "chmod +x ./.ganache-fork.sh && ./.ganache-fork.sh",
    "chain:no_fork": "chmod +x ./.ganache-nofork.sh && ./.ganache-nofork.sh",
    "chain:eth": "node chain-fork.js -eqw",
    "chain:bsc": "node chain-fork.js -bqw"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "solc": "^0.8.6",
    "web3": "^1.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-validator": "^3.0.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.31.0",
    "ganache-cli": "^6.12.2",
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
