{
  "name": "tkngen-api",
  "version": "0.1.0",
  "description": "tkngen's api. This is the backend responsible for making and deploying ERC20 tokens and BEP20 tokens.",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/lib/server.js",
    "start:build": "tsc && node ./dist/lib/server.js",
    "build": "tsc",
    "test": "chmod +x ./.test.sh && ./.test.sh",
    "test:ci": "chmod +x ./.ci-actions.sh && ./.ci-actions.sh",
    "testsuite": "mocha -r ts-node/register tests/.test.ts --timeout 900000",
    "test:coverage": "nyc mocha -r ts-node/register tests/.test.ts --timeout 900000",
    "test:coveralls": "nyc mocha -r ts-node/register tests/.test.ts --timeout 900000 && nyc report --reporter=text-lcov | coveralls",
    "dev:start": "yarn run ts-node ./lib/server.ts",
    "dev:start_hr": "nodemon",
    "chain:fork": "chmod +x ./.ganache/.ganache_fork.sh && ./.ganache/.ganache_fork.sh",
    "chain": "chmod +x ./.ganache/.ganache_nofork.sh && ./.ganache/.ganache_nofork.sh",
    "chain:bsc": "chmod +x ./.ganache/.ganache_bsc.sh && ./.ganache/.ganache_bsc.sh --no_fork",
    "chain:eth": "chmod +x ./.ganache/.ganache_eth.sh && ./.ganache/.ganache_eth.sh --no_fork"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "solc": "^0.8.6",
    "web3": "^1.4.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.21",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-validator": "^3.0.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.31.0",
    "ganache-cli": "^6.12.2",
    "mocha": "^9.0.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
